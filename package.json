{
  "name": "@syngenta-digital/convert-shape-files-to-geojson",
  "version": "DYNAMIC_VERSION",
  "description": "A common package to convert different shape files to geojson",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "",
  "keywords": [],
  "author": "Ram Waykar <Waykar_Rambhau.Pandurang@syngenta.com>",
  "license": "",
  "scripts": {
    "test": "yarn jest --ci --coverage --reporters=default --reporters=jest-junit --max-workers=2 --testTimeout=300000 --verbose",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "build": "yarn lint && yarn tsc --declaration --declarationMap --sourceMap"
  },
  "dependencies": {
    "@mapbox/togeojson": "^0.16.2",
    "jszip": "^3.10.1",
    "proj4": "^2.12.0",
    "shpjs": "^6.1.0",
    "topojson-client": "^3.1.0",
    "wellknown": "^0.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jszip": "^3.4.1",
    "@types/mapbox": "^1.6.45",
    "@types/node": "^20.10.7",
    "@types/proj4": "^2.5.5",
    "@types/shpjs": "^3.4.7",
    "@types/topojson-client": "^3.1.4",
    "@types/wellknown": "^0.5.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "blob-polyfill": "^9.0.20240710",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "tests/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "globals": {
      "Blob": null
    },
    "preset": "ts-jest"
  },
  "files": [
    "dist",
    "src"
  ]
}
